<%= form_for(@documents) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :client %>
  <%= f.text_field :client %>

  <%= f.label :cabinet %>
  <%= f.text_field :cabinet %>

  <%= f.label :localisation %>
  <%= f.text_field :localisation %>

  <%= f.label :year %>
  <%= f.text_field :year %>

  <%= f.label :subject %>
  <%= f.text_field :subject %>

  <%= f.submit %>

<% end %>
